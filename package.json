{
  "name": "diplom",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android": "npm run place-db:android && node node_modules/react-native/local-cli/cli.js run-android",
    "place-db:android": "./place-db.sh android",
    "test": "jest",
    "tsc": "tsc",
    "lint": "tslint --project ./ -t codeFrame"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "react": "16.6.3",
    "react-native": "0.58.6",
    "react-native-sqlite-storage": "^3.3.10",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-saga": "^1.0.2"
  },
  "devDependencies": {
    "@types/jest": "24.0.9",
    "@types/react": "16.8.6",
    "@types/react-native": "0.57.38",
    "@types/react-native-sqlite-storage": "^3.3.1",
    "@types/react-redux": "^7.0.1",
    "@types/react-test-renderer": "16.8.1",
    "@types/redux-actions": "^2.3.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "husky": "^1.3.1",
    "jest": "24.1.0",
    "metro-react-native-babel-preset": "0.53.0",
    "react-test-renderer": "16.6.3",
    "ts-jest": "24.0.0",
    "tslint": "^5.13.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "typescript": "3.3.3333"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  }
}
