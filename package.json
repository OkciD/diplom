{
  "name": "diplom",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "log:android": "node node_modules/react-native/local-cli/cli.js log-android",
    "place-db:android": "./place-db.sh android",
    "test": "jest",
    "tsc": "tsc",
    "lint": "tslint --project ./ -t codeFrame",
    "update-deps": "ncu"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "@react-native-community/slider": "1.1.3",
    "native-base": "2.12.1",
    "react": "16.8.6",
    "react-native": "0.59.8",
    "react-native-easy-router": "2.8.0",
    "react-native-sqlite-storage": "3.3.10",
    "react-native-svg": "9.4.0",
    "react-native-swipe-gestures": "1.0.3",
    "react-redux": "7.0.3",
    "redux": "4.0.1",
    "redux-actions": "2.6.5",
    "redux-saga": "1.0.2"
  },
  "devDependencies": {
    "@types/jest": "24.0.12",
    "@types/react": "16.8.17",
    "@types/react-native": "0.57.53",
    "@types/react-native-sqlite-storage": "3.3.1",
    "@types/react-redux": "7.0.8",
    "@types/react-test-renderer": "16.8.1",
    "@types/redux-actions": "2.6.1",
    "@types/redux-logger": "3.0.7",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.8.0",
    "husky": "2.2.0",
    "jest": "24.8.0",
    "metro-react-native-babel-preset": "0.54.0",
    "npm-check-updates": "3.1.9",
    "react-test-renderer": "16.8.6",
    "redux-logger": "3.0.6",
    "ts-jest": "24.0.2",
    "tslint": "5.16.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "4.0.0",
    "typescript": "3.4.5"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  }
}
